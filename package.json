{
  "packageManager": "yarn@3.8.7",
  "name": "template",
  "version": "1.0.0",
  "description": "Replace this!",
  "main": "build/index.js",
  "typings": "build/types/index.d.ts",
  "author": "Artie Fuzzz",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "build": "tsc -p src",
    "test": "ava",
    "postinstall": "husky install .github/.husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@artiefuzzz/eslint-config": "1.3.1",
    "@artiefuzzz/ts": "1.3.2",
    "@ava/typescript": "5.0.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/cz-commitlint": "19.8.0",
    "@types/node": "22.13.10",
    "ava": "6.2.0",
    "commitizen": "4.3.1",
    "eslint": "9.22.0",
    "husky": "9.1.7",
    "inquirer": "12.4.3",
    "lint-staged": "15.5.0",
    "pinst": "3.0.0",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "./src/**/*.ts": "yarn lint:fix"
  }
}
