{
  "packageManager": "yarn@3.2.1",
  "name": "template",
  "version": "1.0.0",
  "description": "Replace this!",
  "main": "build/index.js",
  "typings": "build/types/index.d.ts",
  "author": "Artie Fuzzz",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "build": "tsc -p src",
    "test": "ava",
    "postinstall": "husky install .github/.husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@artiefuzzz/eslint-config": "1.3.1",
    "@artiefuzzz/ts": "1.3.2",
    "@ava/typescript": "3.0.1",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@commitlint/cz-commitlint": "17.0.3",
    "@types/node": "17.0.45",
    "ava": "4.3.1",
    "commitizen": "4.2.4",
    "eslint": "8.19.0",
    "husky": "8.0.1",
    "inquirer": "8.2.4",
    "lint-staged": "13.0.3",
    "pinst": "3.0.0",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "./src/**/*.ts": "yarn lint:fix"
  }
}
